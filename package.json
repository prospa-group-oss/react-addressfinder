{
  "name": "react-addressfinder",
  "version": "0.3.0",
  "description": "React address lookup component using AddressFinder",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Miymiy",
    "email": "pukapp@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "prebuild": "rm -rf dist",
    "build": "tsc && cp ./src/widget.css ./dist",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "homepage": "https://github.com/prospa-group-oss/react-addressfinder",
  "repository": {
    "type": "git",
    "url": "https://github.com/prospa-group-oss/react-addressfinder.git"
  },
  "keywords": [
    "React",
    "addressfinder",
    "autocomplete"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
